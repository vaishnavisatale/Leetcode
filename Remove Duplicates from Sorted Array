class Solution {
    public int removeDuplicates(int[] nums) 
    {
        //bruit force approach
        //create set and insert element into it and retuen size

        int index=1;
        for(int i=1;i<nums.length;i++)
        {
            if(nums[i-1]!=nums[i] )
            {
                nums[index++]=nums[i];
            }

        }
        return index;
    } 
    
}
